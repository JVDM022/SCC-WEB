# Copy this file to .env and fill in your real Azure Function URLs.
# .env is already ignored by git in this project.

# Needed only for website.py (PostgreSQL)
DATABASE_URL=postgresql://postgres:postgres@127.0.0.1:5432/scc

AZ_TELEMETRY_URL=https://<your-function-app>/api/telemetry?code=<function-key>
AZ_COMMAND_URL=https://<your-function-app>/api/command?code=<function-key>

# Optional
CORS_ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173
AZURE_TIMEOUT_SECONDS=5
PORT=5000
FLASK_DEBUG=0
